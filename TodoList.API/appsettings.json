{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "SqlServer": "YOUR_SQLSERVER_CONNECTION"
  },
  "JWTSettings": {
    "Secret": "YOUR_SECRET_JWT_STRING",
    "Issuer": "http://localhost:5122",
    "Audience": "http://localhost:5122",
    "AccessTokenExpirationMinutes": 15,
    "RefreshTokenExpirationDays": 7
  }
}
